{
  "rust-analyzer.linkedProjects": ["worker/Cargo.toml"],
  "rust-analyzer.checkOnSave.command": "clippy",
  "cSpell.words": [
    "bubbletea",
    "canonicalize",
    "charmbracelet",
    "chrono",
    "cpus",
    "deadpool",
    "deque",
    "dotenv",
    "keepalive",
    "lipgloss",
    "nixos",
    "nixpkgs",
    "numtide",
    "oxalica",
    "Pulumi",
    "reqwest",
    "rustc",
    "rustlib",
    "sqlx",
    "stdenv",
    "tailwindcss",
    "tlog"
  ],
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  }
}
